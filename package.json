{
  "name": "chalmersproject-www",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next dev",
    "build": "next build && next export -o dist",
    "apollo-fetch": "apollo client:download-schema --endpoint http://127.0.0.1:8080/graphql",
    "apollo-generate": "apollo client:codegen --target typescript --outputFlat schema.ts",
    "format": "prettier --write .",
    "lint": "eslint .",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && eslint . --cache --max-warnings 0"
    }
  },
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@chakra-ui/theme-tools": "^1.0.2",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@hookform/error-message": "^0.0.5",
    "@hookform/strictly-typed": "^0.0.4",
    "@rehooks/component-size": "^1.0.3",
    "email-validator": "^2.0.4",
    "firebase": "^8.2.1",
    "framer-motion": "^3.1.1",
    "graphql": "^15.4.0",
    "libphonenumber-js": "^1.9.6",
    "lodash": "^4.17.20",
    "next": "10.0.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-hook-form": "^6.13.1",
    "react-icons": "^4.1.0",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@chakra-ui/react": "^1.0.4",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "apollo": "^2.32.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "git-describe": "^4.0.4",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "standard-version": "^9.0.0",
    "typescript": "^4.1.3"
  }
}
